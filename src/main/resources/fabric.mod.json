{
	"schemaVersion": 1,
	"id": "cookie-debug-mod",
	"version": "${version}",
	"name": "Cookie Debug Mod",
	"description": "This mod adds commands to read and write cookies from/to clients",
	"authors": [
		"TrollyLoki"
	],
	"contact": {
		"sources": "https://github.com/TrollyLoki/cookie-debug-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/cookie-debug-mod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.trollyloki.cookiedebugmod.CookieDebugMod"
		],
		"client": [
			"net.trollyloki.cookiedebugmod.CookieDebugModClient"
		]
	},
	"mixins": [
		"cookie-debug-mod.mixins.json",
		{
			"config": "cookie-debug-mod.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.6",
		"minecraft": "~1.20.5-alpha.24.3.b",
		"java": ">=17",
		"fabric-api": ">=0.95.1+1.20.5"
	}
